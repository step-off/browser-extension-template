{
  "name": "System Chrome Extension",
  "version": "1.0",
  "description": "In memory Chrome Extension",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["*://*.google.com/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }],
  "permissions": ["activeTab","webNavigation"]
}